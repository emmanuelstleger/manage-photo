<form method="post" action="{{ path('photo_delete', {'id': photo.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette photo ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ photo.id) }}">
    <button  class="btn btn-danger btn-sm"> <i class="fa fa-trash"></i> Supprimer</button>
</form>
